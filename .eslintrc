{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "env": {
      "commonjs": true,
      "es6": true,
      "node": true,
      "browser": false,
      "jest": true
  },
  "parserOptions": {
      "ecmaFeatures": {
          "experimentalObjectRestSpread": true,
          "jsx": false
      },
      "sourceType": "module"
  },
  "globals": {
    "strapi": true,
    "Price": true,
    "Location": true,
    "Estateagent": true,
    "Publication": true,
    "Details": true,
    "Description": true,
    "Image": true,
    "Notification": true,
    "Plan": true,
    "Company": true,
    "Authentication": true,
    "Device": true
  },
  "rules": {
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "linebreak-style": ["error", "unix"],
    "no-console": 0,
    "quotes": ["error", "single"],
    "semi": ["error", "always"]
  }
}
